{
  "name": "@floqly/database",
  "version": "0.1.0",
  "private": true,
  "description": "Supabase client and database types for Floqly",
  "main": "./src/index.ts",
  "types": "./src/index.ts",
  "exports": {
    ".": "./src/index.ts",
    "./client": "./src/client.ts",
    "./types": "./src/types.ts"
  },
  "scripts": {
    "generate": "supabase gen types typescript --local > src/types.ts",
    "migrate": "supabase db push",
    "lint": "eslint src/",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf dist node_modules"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.47.2"
  },
  "devDependencies": {
    "@floqly/config": "workspace:*",
    "@types/node": "^25.2.0",
    "eslint": "^9",
    "supabase": "^2.21.0",
    "typescript-eslint": "^8",
    "typescript": "^5"
  }
}
